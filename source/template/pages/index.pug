extends ../_template

block variables
	- var title = "Автосервис в Керчи - Ваш безупречный автосервис | 'Техник автосервис'"
	- var description = "Профессиональный автосервис 'Техник автосервис' в Керчи проведет ремонт и обслуживание автомобиля. Цены и подробности автосервиса в Керчи по тел. +7 (978) 717-87-02"
	- var keywords = "автосервис, авторемонт, автосервис Керчь, обслуживание автомобиля"

block content
	header.header
		.container.header__container
			a(href="./").logo.header__logo
				.logo__img.header__logo-img
					img(src="assets/img/logo.png")
			.address.header__address
				.contact-block.address__contact
					.contact__icon
						svg.contact__svg
							use(xlink:href=`assets/img/sprite.svg#${svgId.where}`)
					.contact__text= contacts.address
				.contact-block.address__contact
					.contact__icon
						svg.contact__svg
							use(xlink:href=`assets/img/sprite.svg#${svgId.phone}`)
					.contact__text= contacts.phone
				.contact-block.address__contact
					.contact__icon
						svg.contact__svg
							use(xlink:href=`assets/img/sprite.svg#${svgId.mess}`)
					.contact__text= contacts.mail
	nav.nav
		.container.nav__container
			.three.nav__three
				div(class="hamburger" id="hamburger-10")
					span(class="line")
					span(class="line")
					span(class="line")
			.menu.nav__menu
				ul.menu__list
					- for(var i = 0; i < menu.text.length; i++)
						li.menu__item
							a(href=`${menu.link[i]}`).menu__link= menu.text[i]
			a(href=`${button.order.link}`).toForm.button.button_blue_border= button.order.text2
	.slideshow
		.slideshow__main
			.slideshow__box.slide-show
				.container.slideshow__container
					.slideshow__body
						h2.slideshow__title
							span.color Бесплатная #{" "}
							| диагностика ходовой
						p.slideshow__text Безопасность стала доступнее! Подробнее о бесплатной диагностике ходовой в нашем автосервисе можно узнать связавшись с нами.
						a(href=`${button.order.linkNow}`).button.slideshow__button.button_blue_border.toForm= button.order.text3
			.slideshow__box
				.container.slideshow__container
					.slideshow__body
						h2.slideshow__title Гарантия качества
						p.slideshow__text Уже более 20 лет наш автосервис работает в сфере ремонта и обслуживания автомобилей в Керчи
						a(href=`${button.order.linkAbout}`).button.slideshow__button.button_blue_border= button.order.about
			.slideshow__arrow.slideshow__arrow_left
				svg.slideshow__svg
					use(xlink:href=`assets/img/sprite.svg#${svgId.arrow}`)
			.slideshow__arrow.slideshow__arrow_right
				svg.slideshow__svg
					use(xlink:href=`assets/img/sprite.svg#${svgId.arrow}`)
	.about
		.container.about__container
			.about__header
				h1.about__title
					| «Техник автосервис» - Ваш
					strong  автосервис в Керчи!
				p.about__text
					| Техник автосервис имеет обширный опыт ремонта и обслуживания автомобилей в Керчи на протяжении более 20 лет. Команда профессионалов нашего автосервиса выявит и устранит любые технические неисправности, подготовит Ваше авто к следующему сезону. В Техник автосервисе в Керчи Вы можете приобрести масла и автохимию по разумным ценам, а также заказать необходимые запчасти. Также нашим постоянным клиентам мы предлагаем систему лояльности. Для Вашего удобства на территории автосервиса есть комфортная комната ожидания, где Вы можете пользоваться интернетом и выпить чашечку кофе.
				a(href=`${button.order.link}`).toForm.button.about__button.button_blue_border= button.order.text
			ul.about__list
					li.about__item
						.about__item__image
							svg.about__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.ecotime}`)
						.about__item__desc
							.about__item__title
								b Экономим
								|  Ваше время
							.about__item__text Мы стараемся делать все работы максимально качественно и быстро
					li.about__item
						.about__item__image
							svg.about__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.prophesy}`)
						.about__item__desc
							.about__item__title
								b Профессиональный
								|  персонал
							.about__item__text Все сотрудники нашего автосервиса - специалисты высокого класса
					li.about__item
						.about__item__image
							svg.about__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.economy}`)
						.about__item__desc
							.about__item__title
								b Бережем
								|  Ваши деньги
							.about__item__text Ценим деньги клиентов и работаем честно, без наценок и переплат
	.amenities
		.triangle.amenities__triangle__left
			svg(viewBox="0 0 1000 500" preserveAspectRatio="none").triangle__left
				polygon(points="0,0 0,500 1000,500")
		.triangle.amenities__triangle__right
			svg(viewBox="0 0 1000 500" preserveAspectRatio="none").triangle__right
				polygon(points="0,500 1000,500 1000,0")
		.container.amenities__container
			h2.amenities__title Все услуги "СТО Техник"
			ul.amenities__list
				- for (var i = 0; i < amen.length; i++)
					li.amenities__item
						a(href=`${amen[i].link}`).amenities__item__link
							.amenities__img
								img(src=`${amen[i].src}` srcset=`${amen[i].srcmini} 300w`)
							.amenities__top
							.amenities__dop
								.amenities__item__title= amen[i].title
								.amenities__item__desc= amen[i].desc
								.button.amenities__button= button.order.about
	.map-block
		.container.map-block__container
			h2.map-block__title Как нас найти
			.map
				.map__map
					script(type="text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3A226bf5e6f1b02a6b3d362d848a027b6444cabe7d6dd9fb881619164c73119136&amp;height=400&amp;lang=ru_RU&amp;scroll=true")
				.map-info
					b.map-info__title Наш адрес
					.contact-block.map__adress
						.contact__icon
							svg.contact__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.where}`)
						.contact__text= contacts.address
					b.map-info__title Время работы
					.contact-block.map__adress
						.contact__icon
							svg.contact__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.times}`)
						.contact__text= contacts.times
					b.map-info__title GPS адрес
					.contact-block.map__adress
						.contact__icon
							svg.contact__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.where}`)
						.contact__text= contacts.gps
			h2.new
				b.line1 Звоните
				b.line2 прямо сейчас
				b.line3 (консультации бесплатно)
	.end
		.send
			.container.send__container
				form(id="form").input-block.send__input-block
					input(type="hidden" name="project_name" value="Сайт автосервиса")
					input(type="hidden" name="admin_email" value="rusinavto@mail.ru")
					input(type="hidden" name="form_subject" value="Заявка на обратный звонок")
					.input.send__input
						.input__before
							svg.input__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.user}`)
						input(type="text" name="Name" placeholder="Ваше имя" required)
					.input.send__input
						.input__before
							svg.input__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.phone}`)
						input(type="text" name="Phone" placeholder="Ваш телефон" required)
					input(type="submit" value=`${button.sends.text2}`).button.send__button.button_blue_bg
		.end__block(id="end")
			.container.end__block__container
				.end__about
					h3.end__title Об автосервисе
					p.end__about__desc
						| Техник автосервис имеет опыт работы в сфере обслуживания автомобилей уже более 20 лет. Мы регулярно обновляем свое оборудование и обучаем персонал работе с современными принципами обслуживания и ремонта автотранспорта.
				.end__amenities
					h3.end__title Наше меню
					.menu.end__menu
						ul.menu__list
							- for(var i = 0; i < menu.text.length; i++)
								li.menu__item
									a(href=`${menu.link[i]}`).menu__link= menu.text[i]
				.end__contacts
					h3.end__title Контакты и время работы
					.contact-block.end__home
						.contact__icon
							svg.contact__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.where}`)
						.contact__text= contacts.address
					.contact-block.end__home
						.contact__icon
							svg.contact__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.phone}`)
						.contact__text= contacts.phone
					.contact-block.end__home
						.contact__icon
							svg.contact__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.mess}`)
						.contact__text= contacts.mail
					.contact-block.end__home
						.contact__icon
							svg.contact__svg
								use(xlink:href=`assets/img/sprite.svg#${svgId.times}`)
						.contact__text= contacts.times